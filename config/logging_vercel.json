{
  "version": 1,
  "disable_existing_loggers": false,
  "formatters": {
    "default": {
      "format": "%(asctime)s %(levelname)s [%(name)s] %(message)s"
    },
    "detailed": {
      "format": "%(asctime)s %(levelname)s [%(name)s:%(lineno)d] %(message)s"
    }
  },
  "handlers": {
    "console": {
      "class": "logging.StreamHandler",
      "level": "DEBUG",
      "formatter": "detailed",
      "stream": "ext://sys.stdout"
    }
  },
  "loggers": {
    "shared.notion_service": {
      "handlers": ["console"],
      "level": "DEBUG",
      "propagate": false
    },
    "autonotion.notion_registry_daily_plan": {
      "handlers": ["console"],
      "level": "DEBUG",
      "propagate": false
    },
    "deployments.vercel.api.run_daily_plan": {
      "handlers": ["console"],
      "level": "DEBUG",
      "propagate": false
    },
    "deployments.vercel.api.scheduled_daily_plan": {
      "handlers": ["console"],
      "level": "DEBUG",
      "propagate": false
    },
    "deployments.vercel.api.hello_notion": {
      "handlers": ["console"],
      "level": "DEBUG",
      "propagate": false
    }
  },
  "root": {
    "level": "DEBUG",
    "handlers": ["console"]
  }
}
